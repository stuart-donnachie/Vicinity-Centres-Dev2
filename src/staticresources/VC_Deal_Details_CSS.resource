 .slds-form-element__control .lookupInput a.slds-input {
     width: auto;
     border: none;
 }
 .slds-input.picklistArrowRight,
 .slds-input.picklistArrowLeft {
     border: none;
     min-height: auto;
     height: 20px;
 }
 .lightening-scope .slds-checkbox .slds-form-element__label {
     font-size: 12px;
     padding-top: 15px;
 }
 .slds-checkbox {
     padding-top: 15px;
 }
 .lookup {
     background: url("{!URLFOR($Resource.icons, 'ic_search_black_24dp_1x.png')}") no-repeat scroll;
     background-position: right 10px center;
     background-color: #fff;
 }
 .slds-form-element__label {
     padding-top: 15px;
 }
 .fleft {
     float: left;
 }
 .clearfix:before,
 .clearfix:after {
     content: " ";
     display: table;
 }
 .clearfix:after {
     clear: both;
 }
 .slds-input__icon {
     cursor: text;
 }
 .slds-multiselect {
     overflow: auto !important;
     padding: 0 !important;
 }
 .sf1select {
     -webkit-appearance: none;
     border: 1px solid #bbc0c4;
     background-image: url(data:image/svg+xml;base64,);
     background-repeat: no-repeat;
     background-position: 95% 50%;
     background-size: 16px 16px, 100% 100%;
     background-color: #fff;
     color: #3c3d3e;
     padding: 7px 14px;
     line-height: 25px;
     font-family: 'ProximaNovaSoft-Regular', 'Helvetica Neue';
     font-size: 15px;
     width: 100%;
     padding: 7px 14px;
     margin: 2px 20px 2px 2px;
     border: 1px solid #cfd0d2;
     border-radius: 5px;
 }
 body {
     background-color: inherit !important;
 }
 .slds-docked-form-footer {
     position: fixed;
     bottom: 0;
     left: 0;
     width: 100%;
     background: #f4f6f9;
     box-shadow: 0 -2px 2px 0 rgba(0, 0, 0, .16);
     z-index: 10000;
     display: -ms-flexbox;
     display: flex;
     -ms-flex-pack: center;
     justify-content: center;
     padding: .5rem 0;
 }
 /*
                .dateInput input{
                width: 89% !important;
                }
                */
 
 .lightening-scope .slds-button--neutral {
     padding-left: 0.7rem !important;
     padding-right: .7rem !important;
 }
 .lightening-scope .slds-notify-container,
 .lightening-scope .slds-notify_container {
     position: relative !important;
     margin-bottom: 10px;
     z-index: 500 !important;
 }
 .lightening-scope .slds-theme--error {
     color: #ffffff;
     background-color: #e09a97;
 }
 .lightening-scope .slds-theme--success {
     color: #ffffff;
     background-color: #a0b39f;
 }
 .slds-form-element__error {
     color: #c23934;
     font-size: .75rem;
     margin-top: .5rem;
     display: block;
 }
 /*
                .slds-required{
                font-size: 1.2rem;
                vertical-align: middle;
                }
                */
 
 .error-list {
     text-align: left !important;
     border: 1px solid #db9693;
     padding: 1rem;
     border-radius: 5px;
     color: #db9693;
 }
 .error-list li {
     list-style: disc !important;
     margin-left: 1rem;
 }
 .error-input {
     /*
                background-color: #ffebea !important;
                border-color: #db9693 !important;
                */
     
     background-color: #fff;
     border-color: #c23934;
     box-shadow: #c23934 0 0 0 1px inset;
     background-clip: padding-box;
 }
 .lightening-scope .slds-select {
     padding: 0 1rem 0 .75rem;
 }
 img.specl_con {
     display: none;
 }
 .padding_right {
     padding-right: 1rem !important;
 }
 .multiSelectPicklistRow select {
     width: 100% !important;
 }
 .multiSelectPicklistCell {
     width: 25px !important;
 }
 .picklistArrowRight {
     width: 25px !important;
 }
 .picklistArrowLeft {
     width: 25px !important;
 }