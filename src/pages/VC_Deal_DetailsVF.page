                                                                    <apex:param value="Area_of_premises_sqmt__c" assignTo="{!refreshSource}" />
                                                                </apex:actionSupport>
                                                        </apex:inputfield>
                                                        <apex:outputfield value="{!objOpp.Area_of_premises_sqmt__c}" styleclass="slds-form-element__static"  rendered="{!IF(!IsEditable,True,False)}"/>
                                                    </div>

                                                    <apex:outputPanel rendered="{!IF(objOpp.Area_of_premises_sqmt__c == null && (errMsg == true || errMsgITL == true),True,False)}">
                                                                <div class="slds-form-element__error" >This field is required</div>
                                                    </apex:outputPanel>
                                                </div>
                                            </apex:outputPanel>

                                            <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                <label class="slds-form-element__label">Leasing Defined Unit Category</label>
                                                <div class="slds-form-element__control">
                                                    <apex:outputfield value="{!objOpp.Unit__r.Current_Tenant_Lease_Status__c}" styleclass="slds-input"/>
                                                </div>
                                            </div>

                                        </div>
