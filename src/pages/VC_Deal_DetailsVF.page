                                            <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                <label class="slds-form-element__label">Deal Type</label>
                                                <div class="slds-form-element__control">
                                                    <apex:outPutfield value="{!objOpp.Deal_Type__c}" styleclass="slds-input"/>
                                                </div>
                                            </div>

                                            <apex:outputPanel layout="none" rendered="{!objOpp.Dev_Clause__c}">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <div class="slds-form-element__control">
                                                        <label class="slds-checkbox">
                                                            <apex:outputField value="{!objOpp.Dev_Clause__c}"/>
                                                            <span class="slds-form-element__label checkbox__label">Development Clause</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>

                                            <apex:outputPanel layout="none" rendered="{!objOpp.Reconfig__c}">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <div class="slds-form-element__control">
                                                        <label class="slds-checkbox">
                                                            <apex:outputField value="{!objOpp.Reconfig__c}"/>
                                                            <span class="slds-form-element__label checkbox__label">Reconfiguration</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>

                                            <apex:outputPanel layout="none" rendered="{!objOpp.Dev_Clause__c}">

                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <div class="slds-form-element__control">
                                                        <label class="slds-checkbox">
                                                            <apex:outputField value="{!objOpp.Development_Unit__c}"/>
                                                            <span class="slds-form-element__label checkbox__label">Development Unit</span>
                                                        </label>
                                                    </div>
                                                </div>

                                            </apex:outputPanel>

                                            <apex:outputPanel layout="none" rendered="{!objOpp.Dev_Clause__c}">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <div class="slds-form-element__control">
                                                        <label class="slds-checkbox">
                                                            <apex:outputField value="{!objOpp.Dev_Budget__c}"/>
                                                            <span class="slds-form-element__label checkbox__label">Development Budget</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>

                                            <apex:outputPanel layout="none" rendered="{!objOpp.Early_Renewal__c}">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <div class="slds-form-element__control">
                                                        <label class="slds-checkbox">
                                                            <apex:outputField value="{!objOpp.Early_Renewal__c}"/>
                                                            <span class="slds-form-element__label checkbox__label">Early Renewal</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>

                                            <apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4t'}" layout="none">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <label class="slds-form-element__label">Deal Name</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputfield value="{!objOpp.Name}" styleclass="slds-input"/>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                           
                                            <apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4t'}" layout="none">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <label class="slds-form-element__label">Centre</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputfield value="{!objOpp.Unit__r.Building__r.Centre__c}" styleclass="slds-input"/>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4t'}" layout="none">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <label class="slds-form-element__label">Building</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputfield value="{!objOpp.Unit__r.Building__c}" styleclass="slds-input"/>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>

                                            <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                <label class="slds-form-element__label">Unit</label>
                                                <div class="slds-form-element__control">
                                                    <apex:outputfield value="{!objOpp.Unit__c}" styleclass="slds-input"/>
                                                </div>
                                            </div>

                                            <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                <label class="slds-form-element__label">Unit Type</label>
                                                <div class="slds-form-element__control">
                                                    <apex:outputfield value="{!objOpp.Unit__r.Unit_Type__c}" styleclass="slds-input"/>
                                                </div>
                                            </div>

                                            <apex:outputPanel layout="none" rendered="{!IF(objOpp.Development_Unit__c == false && objOpp.Reconfig__c == false,True,False)}">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                                                    <label class="slds-form-element__label">Area of premises (sqm)</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputfield value="{!objOpp.Area_of_premises_sqmt__c}" styleclass="slds-input"/>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>

                                            <apex:outputPanel layout="none" rendered="{!IF(objOpp.Development_Unit__c == true || objOpp.Reconfig__c == true,True,False)}">
                                                <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 {!IF($User.UIThemeDisplayed != 'Theme4t','padding_right','padding_right')}">
                                                    <label class="slds-form-element__label"><span class="slds-required " title="required">*</span> Area of premises (sqm)</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:inputfield value="{!objOpp.Area_of_premises_sqmt__c}" styleclass="slds-input {!IF(objOpp.Area_of_premises_sqmt__c == null && (errMsg == true || errMsgITL == true),'error-input','')}"  rendered="{!IF(IsEditable,True,False)}">
