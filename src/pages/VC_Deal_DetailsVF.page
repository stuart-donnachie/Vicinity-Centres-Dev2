            function setLookUp(field, remote_action) {
                $('#lookup_' + field).keyup(function(e) {
                    if ($('#lookup_' + field).val() != '') {
                        $('#results_' + field).css("display", "block");
                        $('#lookup_' + field).attr('aria-expanded', 'true');
                        $('#keyword_' + field).html($('#lookup_' + field).val());
                        Visualforce.remoting.Manager.invokeAction(
                            remote_action,
                            $('#lookup_' + field).val(),
                            '{!objOpp.Unit__r.Building__c}',
                            function(result, event) {
                                if (event.status) {
                                    $('#ui_results_' + field).empty();
                                    populateLookup(result);
                                } else if (event.type === 'exception') {
                                    $('#lookup_' + field) = 'ERROR: ' + event.message;
                                } else {
                                    $('#lookup_' + field) = 'ERROR: ' + event.message;
                                }
                            }, {
                                escape: true
                            }
                        );

                    } else {
                        $('#results_' + field).css("display", "none");
                    }
                });

                function populateLookup(result) {
                    if (result) {
                        $(result).each(function() {
                            console.log(this);
                            $('#ui_results_'+field).append('<li class="slds-lookup__item">' +
                                                           '<a id="' + this.Id + '" href="javascript:void(0)" role="option">' +
                                                           '<svg aria-hidden="true" class="slds-icon slds-icon-standard-account slds-icon--small">' +
                                                           '<use xlink:href="{!URLFOR ($Resource.SalesforceLighteningDesignSystem, 'assets/icons/utility-sprite/svg/symbols.svg#company')}"></use>' +
                                                           '</svg>' + this.Name + '</a>' +
                                                           '</li>');
                        });

                        $('ul.slds-lookup__list_' + field + ' a').click(function(e) {
                            $('#lookup_' + field).val($(this)[0].innerText);
                            $('#lookup_' + field).attr('aria-activedescendant', $(this)[0].id);
                            $('#lookup_' + field).attr('aria-expanded', 'false');
                            $('#results_' + field).css("display", "none");
                            $('[id$=hidden_' + field + ']').val($(this)[0].id);
                        });
                    }
                }
            }

            $(document).ready(function() {
