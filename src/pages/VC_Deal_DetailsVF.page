<apex:page id="thePage" standardcontroller="opportunity" extensions="VC_Deal_DetailsController,VC_DealsCommon" sidebar="false"
           showHeader="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0" cache="true"  standardStylesheets="true">
    <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">
        <head>
            <meta charset="utf-8" />
            <meta http-equiv="x-ua-compatible" content="ie=edge" />
            <title>Deal</title>
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <apex:stylesheet value="{!URLFOR($Resource.SalesforceLighteningDesignSystem, '/assets/styles/salesforce-lightning-design-system-vf.min.css')}" />
        </head>
        <body>
            <style>
             .slds-form-element__control .lookupInput a.slds-input{width:auto;border:none;}
            .slds-input.picklistArrowRight,.slds-input.picklistArrowLeft{border:none;min-height:auto;height:20px;}
            .lightening-scope .slds-checkbox .slds-form-element__label{font-size:12px;padding-top:15px;}
            .lookup{background:url("{!URLFOR($Resource.icons, 'ic_search_black_24dp_1x.png')}") no-repeat scroll;background-position:right 10px center;background-color:#fff;}
            .fleft{float:left;}
            .clearfix:before,.clearfix:after{content:" ";display:table;}
            .clearfix:after{clear:both;}
            .slds-input__icon{cursor:text;}
            .slds-multiselect{overflow:auto!important;padding:0!important;}
            .sf1select{-webkit-appearance:none;background-image:url(data:image/svg+xml;base64,);background-repeat:no-repeat;background-position:95% 50%;background-size:16px 16px 100% 100%;background-color:#fff;color:#3c3d3e;line-height:25px;font-family:ProximaNovaSoft-Regular, 'Helvetica Neue';font-size:15px;width:100%;border:1px solid #cfd0d2;border-radius:5px;margin:2px 20px 2px 2px;padding:7px 14px;}
            body{background-color:inherit!important;}
            .slds-docked-form-footer{position:fixed;bottom:0;left:0;width:100%;background:#f4f6f9;box-shadow:0 -2px 2px 0 rgba(0,0,0,.16);z-index:10000;display:flex;-ms-flex-pack:center;justify-content:center;padding:.5rem 0;}
            .lightening-scope .slds-button--neutral{padding-left:.7rem!important;padding-right:.7rem!important;}
            .lightening-scope .slds-notify-container,.lightening-scope .slds-notify_container{position:relative!important;margin-bottom:10px;z-index:500!important;}
            .lightening-scope .slds-theme--error{color:#fff;background-color:#e09a97;}
            .lightening-scope .slds-theme--success{color:#fff;background-color:#a0b39f;}
            .slds-form-element__error{color:#c23934;font-size:.75rem;margin-top:.5rem;display:block;}
            .error-list{text-align:left!important;border:1px solid #db9693;border-radius:5px;color:#db9693;padding:1rem;}
            .error-list li{list-style:disc!important;margin-left:1rem;}
            .error-input{background-color:#fff;box-shadow:#c23934 0 0 0 1px inset;background-clip:padding-box;border-color:#c23934;}
            .lightening-scope .slds-select{padding:0 1rem 0 .75rem;}
            img.specl_con{display:none;}
            .padding_right{padding-right:1rem!important;}
            .multiSelectPicklistRow select{width:100%!important;}
            .slds-checkbox,.slds-form-element__label{padding-top:15px;}
            .multiSelectPicklistCell,.picklistArrowRight,.picklistArrowLeft{width:25px!important;}

            <apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4t'}" layout="none">
                .datePicker{display:none;}
            </apex:outputPanel>
            <apex:outputPanel rendered="{!$User.UIThemeDisplayed != 'Theme4t'}" layout="none">
                .slds-docked-form-footer{padding-bottom: 0;justify-content: flex-start;}
                .lightening-scope .slds-select_container:before{border: none;}
                .lightening-scope .slds-select_container:after{ border: none;}
            </apex:outputPanel>
            </style>
            <apex:form id="theForm">
                {!debug}
                <div class="lightening-scope">

                    <div class="slds-spinner1 slds-spinner_container slds-hide">
                        <div class="slds-spinner--brand slds-spinner slds-spinner--medium" aria-hidden="false" role="alert">
                            <div class="slds-spinner__dot-a"></div>
                            <div class="slds-spinner__dot-b"></div>
                        </div>
                    </div>

                    <apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4t'}">
                        <div class="slds-page-header" role="banner">
                            <div class="slds-media">
                                <div class="slds-media__figure">
                                    <!--
                                    <svg aria-hidden="true" class="slds-icon slds-icon-standard-opportunity">
                                    <use xlink:href="{!URLFOR($Resource.SalesforceLighteningDesignSystem, '/assets/icons/standard-sprite/svg/symbols.svg#account')}"></use>
                                    </svg>
                                    -->
                                    <apex:image value="{!URLFOR($Resource.icons, 'account_60.png')}" styleClass="slds-icon slds-icon-standard-opportunity"/>
                                </div>
                                <div class="slds-media__body">
                                    <p class="slds-page-header__title slds-truncate" >Deal</p>
                                    <p class="slds-text-body--small">{!objOpp.Name}</p>
                                </div>
                            </div>
                        </div>

                    </apex:outputPanel>
                    <div class="slds-tabs--default">
                        <ul class="slds-tabs--default__nav" role="tablist">
                            <li class="slds-tabs--default__item slds-active" title="Part I" role="presentation"><a class="slds-tabs--default__link" href="javascript:void(0);" role="tab" tabindex="0" aria-selected="true" aria-controls="tab-default-1" id="tab-default-1__item" onclick="selectTab(this);">Part I</a></li>
                            <li class="slds-tabs--default__item" title="Part II" role="presentation"><a class="slds-tabs--default__link" href="javascript:void(0);" role="tab" tabindex="-1" aria-selected="false" aria-controls="tab-default-2" id="tab-default-2__item" onclick="selectTab(this);">Part II</a></li>
                        </ul>
                        <br/>
                        <div class="slds-box slds-theme--shade slds-theme--alert-texture">
                            <div>
                                <span>Deal Stage: <strong>{!objOpp.StageName}</strong></span>
                                <!-- Stuart Donnachie -- Trying to work out a way for them to recreate the TDP if it has failed -->
                                <!--<span><apex:commandButton action="{!recreateTDP}" value="Retry TDP Creation" styleclass="slds-button slds-button&#45;&#45;brand" onclick="showSpinner();" status="status" rendered="{!IF(objOpp.TDP_Created__c == false && objOpp.Existing_Retailer__c == true,True,False)}"/></span>-->
                                <span style="float:right" class="slds-pill slds-pill_link">
                                  <span class="slds-pill__icon_container" title="Account" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                          <svg class="slds-icon" aria-hidden="true">
                                            <use xlink:href="{!URLFOR($Resource.SalesforceLighteningDesignSystem, '/assets/icons/doctype-sprite/svg/symbols.svg#link')}"></use>
                                          </svg>
                                          <span class="slds-assistive-text">Account</span>
                                      </span>
                                  <a href="http://ecmwebau:8080/webtop/servlet/FolderOpen?SFID={!objOpp.Deal_Key__c}&TransType=LON" class="slds-pill__action">
                                    <span class="slds-pill__label">ECM LON Process Folder  </span>
                                  </a>
                                </span>
                            </div>
                        </div>
                        <div id="tab-default-1" class="slds-tabs--default__content slds-show" role="tabpanel" aria-labelledby="tab-default-1__item">
                            <!-- - -->
                            <apex:outputPanel id="thePage1" layout="none">
                                <div class="slds-grid slds-grid--align-space">
                                    <div class="slds-col--padded slds-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-1">

                                       <apex:outputPanel id="errMsg5x" >
                                            <apex:outputPanel rendered="{!objOpp.Is_Incomplete__c || objOpp.API_Error__c}">
                                                <div class="slds-notify_container">
                                                    <div class="slds-notify slds-notify--alert slds-theme--error slds-theme--alert-texture" role="alert">
                                                        <span class="slds-assistive-text">Error</span>
                                                        <h2>An error occurred during the deal creation. Please contact the administrator.</h2>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                        </apex:outputPanel>

                                        <apex:outputPanel id="errMsg5a" >
                                            <apex:outputPanel rendered="{!IF((errMsg == true || errMsgITL == true),True,False)}">
                                                <div class="slds-notify_container">
                                                    <div class="slds-notify slds-notify--alert slds-theme--error slds-theme--alert-texture" role="alert">
                                                        <span class="slds-assistive-text">Error</span>
                                                        <h2>Please review the errors</h2>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                        </apex:outputPanel>

                                        <apex:outputPanel id="loifMsga" >
                                            <apex:outputPanel rendered="{!IF(lonGenerated == true,True,False)}">
                                                <div class="slds-notify_container">
                                                    <div class="slds-notify slds-notify--alert slds-theme--success slds-theme--alert-texture" role="alert">
                                                        <span class="slds-assistive-text">LON Generated</span>
                                                        <h2>Successfully generated the LON. Please check your email.</h2>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                        </apex:outputPanel>

                                        <apex:outputPanel id="eoiMsga" >
                                            <apex:outputPanel rendered="{!IF(eoiGenerated == true,True,False)}">
                                                <div class="slds-notify_container">
                                                    <div class="slds-notify slds-notify--alert slds-theme--success slds-theme--alert-texture" role="alert">
                                                        <span class="slds-assistive-text">EOI Generated</span>
                                                        <h2>Successfully generated the EOI. Please check your email.</h2>
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                        </apex:outputPanel>

                                        <apex:outputPanel id="messagePanel1" rendered="{!errMsg == true || errMsgITL == true}">
