                                                <apex:messages id="messages1" styleClass="errorM3"/>
                                            </div>
                                        </apex:outputPanel>

                                        <apex:outputPanel id="messagePanel3" rendered="{!errMsg != true && errMsgITL != true}">
                                            <apex:messages id="messages3" styleClass="errorM3" style="color: red; border: solid 1px red; padding: 1rem; margin-bottom: 1rem;"/>
                                        </apex:outputPanel>

                                        <h3 class="slds-section-title--divider" style="background: #e0e5ee;font-weight:bold; font-size: 0.9rem;">Key Information</h3>

