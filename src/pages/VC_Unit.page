        <body>
            
            <apex:form id="mainForm">
                <style>
                	<apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4t'}" layout="none"> 
                    .datePicker{
                    display:none;
                    }
                	</apex:outputPanel>
                    .lightening-scope .slds-card {background-color: #fff !important;}
                    .slds-card__body{padding: .75rem;}
                    .xlookup input.slds-input{
                    width: 92% !important;
                    }
                    
                    .slds-form-element__control .lookupInput a.slds-input{
                    width: auto;
                    border: none;
                    }
                    .msgIcon {
                    display: none!important
                    }
                    .customMessage * {
                    color: #fff!important
                    }
                    .customMessage {
                    margin: 5px 0!important;
                    /* max-width: 1280px; */
                    opacity: 1!important;
                    width: 100%;
                    font-size: 12px;
                    border: 0px;
                    padding-left: 10px;
                    }
                    .message {
                    opacity: .1
                    }
                    .slds-notify_container{
                    position: static !important;
                    }
                    .lightening-scope .slds-form-element {
                    line-height: 2.5;
                    }
                    .slds-form-element{
                    margin-top: .5rem;
                    }
                    
                    .errorM3 {
                    color: #fff !important;
                    background-color: #c23934 !important;
                    padding: .5rem;
                    text-align: center;
                    position: relative;
                    font-weight: 300;
                    line-height: 2.5em;
                    }
                    .lightening-scope .slds-spinner_container{
                    z-index: 99999999999 !important;
                    }
                    h1 {
				    display: block;
				    font-size: 2em;
				    -webkit-margin-before: 0.67em;
				    -webkit-margin-after: 0.67em;
				    -webkit-margin-start: 0px;
				    -webkit-margin-end: 0px;
				    font-weight: bold;
				}
				
				
